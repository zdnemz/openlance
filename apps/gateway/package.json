{
  "name": "@openlance/gateway",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build ./src/index.ts --outdir ./dist --target bun",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.1.1",
    "@elysiajs/swagger": "^1.3.1",
    "@openlance/cache": "*",
    "@openlance/logger": "*",
    "@openlance/shared": "*",
    "elysia": "^1.2.10"
  },
  "devDependencies": {
    "@openlance/config": "*",
    "@types/bun": "^1.1.14",
    "typescript": "^5.7.3"
  }
}
